<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="JdbcSaveController"
            prefHeight="100" prefWidth="200">

    <HBox spacing="10">
        <Label prefHeight="25.0" prefWidth="80.0" text="%gameName"/>
        <TextField fx:id="gameName" promptText="%gameNamePrompt"/>
        <Button onAction="#saveToDatabase" text="%dbSaveProceed"/>
    </HBox>
</AnchorPane>
